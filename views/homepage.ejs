<div data-ng-app="RealTimeChat" data-ng-controller="ChatController" data-ng-init="init()">
  <ng-container>
      <div class="row" ng-repeat="chat in chatList">
        {{chat.username}}: {{chat.message}}
      </div>
      <div class="row">
        <div class=col-md-4>
          <input type="text" data-ng-model="name" />
        </div>
        <div class="col-md-4">
          <input type="text" data-ng-model="message"/>
        </div>
        <div class="col-md-4">
            <button type="button" class="btn btn-sm" ng-click="submit()">Submit</button>
        </div>
      </div>
  </ng-view>
</div>